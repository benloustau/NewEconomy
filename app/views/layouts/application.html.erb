<!DOCTYPE html>
<html>
<head>
  <title>NewEconomy</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <script src="//maps.google.com/maps/api/js?v=3.13&amp;sensor=false&amp;libraries=geometry" type="text/javascript"></script>
<script src='//google-maps-utility-library-v3.googlecode.com/svn/tags/markerclustererplus/2.0.14/src/markerclusterer_packed.js' type='text/javascript'></script>
<link href='http://fonts.googleapis.com/css?family=Julius+Sans+One' rel='stylesheet' type='text/css'>
  <link href='http://fonts.googleapis.com/css?family=Lora:400,700' rel='stylesheet' type='text/css'>
  <link href='http://fonts.googleapis.com/css?family=Open+Sans+Condensed:light&v1' rel='stylesheet' type='text/css'>

  <%= csrf_meta_tags %>
</head>
<body>
	<header>
 		<div id="banner">
        	<div id="new_economy">
        		<div id="new">New</div>
        		<div id="logo_mini">
              <div id="nyc">NYC</div>
            </div>
        		<div id="economy">Economy</div>
        	
    		</div>

		</div>
	</header>
	<% if !current_page?('/') && current_user %>

		<nav class="nav">
				<ul>
					<li><%= link_to "Edit Profile", edit_user_path(current_user) %></li>
					<li><%= link_to 'All Users', users_path, method: :get %></li>
					<li><%= link_to 'Sign out', destroy_user_session_path, method: :delete %></li>
				</ul>	
		</nav>

	<% end %>


<%= yield %>

</body>
</html>
